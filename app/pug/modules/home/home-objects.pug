section.home_objects
    .container
        h2.block-title Объекты
        .home_objects_slider
            each val in [1, 2, 3]
                each val in [1, 2, 3, 4, 5, 6]
                    .home_objects_col
                        .home_objects_item(style='background-image: url(img/home/objects/object' + val + '.png)')
                            .home_objects_item_description
                                .address
                                    span ул. Генуэзская,1 Одесса
                                .name ЖК «РОДОС»
                                .price от 21 990 грн/м²
                                a.home_objects_item_description_details(href='/single-object.html') Подробнее
                            .home_objects_item_label В продаже